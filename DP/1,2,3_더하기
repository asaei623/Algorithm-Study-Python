def dp(n):
    # 1. 테이블 정의하기
    d = [None] * (n+1)
    # 2. 점화식 정의하기; d[k] = d[k-1] + d[k-2] + d[k-3]
    # 3. 초기값 정의하기
    d[1] = 1
    d[2] = 2
    d[3] = 4
    # 4. 테이블 채우기
    for i in range(4, n+1):
        d[i] = d[i-1] + d[i-2] + d[i-3]
    return d[n]
    


# 입력
t = int(input())
for _ in range(t):
    n = int(input())
    # 출력
    print(dp(n))
